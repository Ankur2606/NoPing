# Gmail API OAuth Credentials
CREDENTIALS_PATH="./credentials.json"

# Firebase Configuration
GOOGLE_APPLICATION_CREDENTIALS="./config/serviceAccountKey.json"
FIREBASE_DATABASE_URL="https://your-project-id.firebaseio.com"

# Individual Firebase credentials (alternative to GOOGLE_APPLICATION_CREDENTIALS)
FIREBASE_PROJECT_ID="your-project-id"
FIREBASE_CLIENT_EMAIL="your-service-account-email@your-project-id.iam.gserviceaccount.com"
FIREBASE_PRIVATE_KEY="your-private-key-here"

# Nebius AI API Key for Qwen 2.5 model access
NEBIUS_API_KEY=your-nebius-api-key-here

# User-specific settings
DEFAULT_USER_ID="your-user-id"

# Application settings
FETCH_INTERVAL=7200000  # Email fetch interval in milliseconds (default: 2 hours)